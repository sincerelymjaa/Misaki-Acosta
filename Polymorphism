# This is Polymorphism in OOP
class Album:
        def print_version (self):
            print ("CD Album Version")
# Album is the parent class

class KPopAlbum(Album):
      def print_version (self):
            print ("K-Pop Photobook Album Version")

class JPopAlbum(Album):
      def print_version (self):
            print ("J-Pop CD Album Version")

class DisplayAlbum(Album):
      def view (self):
            print ("This is the Display Album")
                
al1 = KPopAlbum()
al2 = JPopAlbum()
al3 = DisplayAlbum()

al1.print_version()
al2.print_version()
al3.print_version()
